    "metalsmith-in-place": "^4.1.1",
    "metalsmith-metallic": "^2.0.2",
    "metalsmith-prism": "^3.1.1",
    "metalsmith-project-images": "^1.1.2",
    "metalsmith-publish": "^0.1.5",
    "metalsmith-replace": "^1.0.0",
